# OpenRouter API Key (required)
REACT_APP_OPENROUTER_API_KEY=sk-or-v1-...

# AI Model Stack (V3.4 Safety-Hardened)
# Director: logic & strategy
REACT_APP_DIRECTOR_MODEL=xiaomi/mimo-v2-flash:free
# Actor: uncensored dialogue
REACT_APP_ACTOR_MODEL=nousresearch/hermes-3-llama-3.1-405b:free
# Backup: emergency safety net (Chimera)
REACT_APP_BACKUP_MODEL=deepseek/deepseek-r1:free

# Optional: Enable verbose AI debug logging
# REACT_APP_AI_DEBUG=true

# Optional: Embedding model (1536 dimensions)
# REACT_APP_OPENROUTER_EMBEDDING_MODEL=text-embedding-3-small

# Supabase (optional; if you want logging/memories)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-anon-key
